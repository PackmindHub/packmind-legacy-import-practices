{
  "name": "packmind-legacy-import",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun build ./src/index.ts --compile --outfile ./dist/packmind-legacy-import",
    "build:minify": "bun build ./src/index.ts --compile --minify --outfile ./dist/packmind-legacy-import",
    "build:linux": "bun build ./src/index.ts --compile --target=bun-linux-x64 --outfile ./dist/packmind-legacy-import-linux",
    "build:windows": "bun build ./src/index.ts --compile --target=bun-windows-x64 --outfile ./dist/packmind-legacy-import-windows",
    "start": "./dist/packmind-legacy-import",
    "dev": "bun run --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "engines": {
    "node": ">=22.17.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "openai": "^4.73.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "latest",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "eslint": "^9.39.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.48.0"
  }
}
